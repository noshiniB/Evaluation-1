    "def count_the_vowels(txt):\n",
    "    txt1 = txt.lower()\n",
    "    list1 = [\"a\",\"e\",\"i\",\"o\",\"u\"]\n",
    "    vowels = 0\n",
    "    for char in txt1:\n",
    "        if char in list1:\n",
    "            vowels = vowels + 1\n",
    "    return vowels\n",
    "print(count_the_vowels(\"NOSHINI\"))\n"
   ]
  }
